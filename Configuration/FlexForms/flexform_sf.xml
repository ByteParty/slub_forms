<T3DataStructure>
<meta>
    <langDisable>1</langDisable>
</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Select function</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Show Form</numIndex>
										<numIndex index="1">Email->new;Email->create</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>

					<settings.formsSelection>
						<TCEforms>
							<label>Show following Forms:</label>
							<config>
								<type>select</type>
								<renderMode>tree</renderMode>
								<treeConfig>
									<parentField>parent</parentField>
									<appearance>
										<expandAll>TRUE</expandAll>
										<showHeader>TRUE</showHeader>
									</appearance>
								</treeConfig>
								<foreign_table>tx_slubforms_domain_model_forms</foreign_table>
								<foreign_table_where>AND tx_slubforms_domain_model_forms.pid=###STORAGE_PID### AND (tx_slubforms_domain_model_forms.sys_language_uid = 0 OR tx_slubforms_domain_model_forms.l10n_parent = 0) ORDER BY tx_slubforms_domain_model_forms.sorting</foreign_table_where>
								<size>5</size>
								<autoSizeMax>30</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
							</config>
						</TCEforms>
					</settings.formsSelection>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
